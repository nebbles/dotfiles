{
    "jest.shell": "/bin/zsh",
    "jest.jestCommandLine": "source ~/.zshrc && nvm i && yarn test",
    "jest.runMode": "on-demand",
    "testing.openTesting": "neverOpen",
    // inheritEnv usually defaults to true. This was changed to make conda work properly for the integrated terminal.
    "terminal.integrated.inheritEnv": false,
    "telemetry.enableTelemetry": false,
    "workbench.colorTheme": "Default Light Modern",
    "atomKeymap.promptV3Features": true,
    "editor.wordWrap": "on",
    "[restructuredtext]": {
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "restructuredtext.linter.extraArgs": [
        "--ignore D001"
    ],
    "sync.gist": "9e3e4e2edfb0542def8dbe6238715d21",
    "sync.autoUpload": true,
    "editor.minimap.enabled": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "git.autofetch": true,
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "editor.rulers": [
        80,
        100,
        120
    ],
    "window.restoreWindows": "all",
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "liveshare.featureSet": "stable",
    "gitlens.defaultDateShortFormat": "D MMM, YYYY",
    "gitlens.defaultDateFormat": "Do MMMM, YYYY at hh:mm",
    "editor.formatOnSave": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnPaste": false
    },
    "python.formatting.provider": "black",
    "editor.formatOnPaste": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "C_Cpp.clang_format_path": "${workspaceRoot}/bin/clang-format/darwin_x64/clang-format",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "rewrap.wrappingColumn": 80,
    "todo-tree.highlights.enabled": false,
    "plantuml.render": "PlantUMLServer",
    "plantuml.server": "https://www.plantuml.com/plantuml",
    "workbench.iconTheme": "vscode-icons",
    "todo-tree.tree.showScanModeButton": false,
    "githubPullRequests.queries": [
        {
            "label": "Waiting For My Review",
            "query": "repo:${owner}/${repository} is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "repo:${owner}/${repository} is:open assignee:${user}"
        },
        {
            "label": "Created By Me",
            "query": "repo:${owner}/${repository} is:open author:${user}"
        },
        {
            "label": "Mentioned Me",
            "query": "repo:${owner}/${repository} is:open mentions:${user}"
        }
    ],
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.confirmSync": false,
    "taskExplorer.enableSideBar": true,
    "taskExplorer.pathToMake": "make",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.tabs.enabled": true,
    "sync.quietSync": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb",
        "git-rebase-todo": "default"
    },
    "terminal.integrated.scrollback": 50000,
    "redhat.telemetry.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "yaml": false
    },
    "workbench.panel.defaultLocation": "right",
    "comments.openView": "never",
    "vsicons.dontShowNewVersionMessage": true,
    "terminal.external.osxExec": "Warp.app",
    "githubPullRequests.pullBranch": "never",
    "remoteHub.gitProtocol": "ssh",
    "terminal.integrated.persistentSessionScrollback": 1000,
    "jupyter.interactiveWindow.textEditor.executeSelection": false,
    "go.toolsManagement.autoUpdate": true,
    "editor.accessibilitySupport": "off",
    "githubPullRequests.terminalLinksHandler": "github",
    "gitlens.rebaseEditor.ordering": "asc",
    "editor.stickyScroll.enabled": true,
    "workbench.preferredDarkColorTheme": "One Dark Pro",
    "window.autoDetectColorScheme": true,
    "prisma.showPrismaDataPlatformNotification": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.codeActionsOnSave.rules": null,
    "[git-commit]": {
        "rewrap.wrappingColumn": 72
    },
    "testing.automaticallyOpenTestResults": "neverOpen",
    "database-client.autoSync": true,
    "gitlens.views.scm.grouped.views": {
        "commits": true,
        "branches": true,
        "remotes": true,
        "stashes": false,
        "tags": true,
        "worktrees": true,
        "contributors": true,
        "repositories": false,
        "searchAndCompare": false,
        "launchpad": false
    },
    "chat.agent.enabled": true,
    "github.copilot.nextEditSuggestions.enabled": true
}